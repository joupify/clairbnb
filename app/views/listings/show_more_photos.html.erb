<!-- app/views/listings/show_more_photos.html.erb -->
<div class="container py-2 mt-5">
    <h2>More Photos</h2>
    <div class="row">
      <% @additional_photos.each do |photo| %>
        <div class="col-6 mb-2">
          <%= image_tag photo.image.url, class: 'd-block w-100' if photo.image.present? %>
        </div>
      <% end %>
    </div>
    <%= link_to 'Back to listing', listing_path(@listing), class: 'btn btn-secondary mt-3' %>
  </div>
  